// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Farmer {
  id             String         @id @default(uuid())
  name           String         @unique
  farmName       String
  city           String
  state          String
  arableArea     Int
  vegetationArea Int
  identifier     String
  foodsPlanted   FoodsPlanted[]

  @@map("public.farmer")
}

model FoodsPlanted {
  id       String @id @default(uuid())
  name     String
  quantity Int
  farmerId String
  Farmer   Farmer @relation(fields: [farmerId], references: [id])
}

